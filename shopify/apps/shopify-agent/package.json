{
  "name": "shopify-agent-features",
  "version": "1.0.0",
  "description": "Shopify Agent Commerce integration package",
  "type": "module",
  "main": "api_server.py",
  "scripts": {
    "start": "python api_server.py",
    "dev": "uvicorn api_server:app --reload --host 0.0.0.0 --port 8000",
    "test": "python -m pytest tests/",
