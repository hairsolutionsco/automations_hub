{
  "name": "n8n-automation",
  "version": "1.0.0",
  "description": "N8N automation tools and workflows",
  "type": "module",
  "scripts": {
    "start": "n8n start",
    "start:tunnel": "n8n start --tunnel",
    "dev": "n8n start --tunnel",
    "export": "n8n export:workflow --all --output workflows --separate",
    "import": "n8n import:workflow --input workflows --separate",
    "cloud:export": "n8n export:workflow --all --output workflows --separate --baseUrl $N8N_CLOUD_INSTANCE_URL --userEmail $N8N_USER_EMAIL --password $N8N_API_KEY",
    "cloud:import": "n8n import:workflow --input workflows --separate --baseUrl $N8N_CLOUD_INSTANCE_URL --userEmail $N8N_USER_EMAIL --password $N8N_API_KEY"
  },
  "dependencies": {
    "n8n": "^1.65.0"
  },
  "devDependencies": {}
}
